<?xml version="1.0"?>
<launch>
  <arg name="use_sim_time" default="true"/>
  <arg name="robot_urdf_path" default="$(find fetch_description)/robots/fetch.urdf.xacro"/>
  <param name="/use_sim_time" value="$(arg use_sim_time)"/>


  <!-- run the coppeliasim -->
  <!-- <node name="start_coppeliasim" pkg="coppeliasim_run" type="start_coppeliasim" args=" -s $(find fetch_coppeliasim)/scene/pullpush.ttt" output="screen"/> -->
  <node name="start_coppeliasim" pkg="coppeliasim_run" type="start_coppeliasim" args=" -s $(find fetch_coppeliasim)/scene/first_floor_jh.ttt" output="screen"/>

  <!-- send robot urdf to param server -->
  <param name="robot_description" command="$(find xacro)/xacro $(arg robot_urdf_path)" />
  <!-- <node pkg="tf" type="static_transform_publisher" name="world_2_odom_broadcaster" args="0 0 0 0 0 0 1 world map 100" /> -->

</launch>
